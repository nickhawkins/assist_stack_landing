{
  "name": "landing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/input.css -o ./docs/output.css --minify",
    "build:html": "cp ./src/index.html ./docs/index.html",
    "build:hero": "cp ./src/hero.webp ./docs/hero.webp",
    "build:chatbot": "cp ./src/chatbot.webp ./docs/chatbot.webp",
    "build:feature-1": "cp ./src/feature-1.webp ./docs/feature-1.webp",
    "build:feature-2": "cp ./src/feature-2.webp ./docs/feature-2.webp",
    "build:feature-3": "cp ./src/feature-3.webp ./docs/feature-3.webp",
    "build:logo": "cp ./src/logo.webp ./docs/logo.webp",  
    "build:logo_1": "cp ./src/logo_1.webp ./docs/logo_1.webp",  
    "build:logo_2": "cp ./src/logo_2.webp ./docs/logo_2.webp", 
    "build:logo_3": "cp ./src/logo_3.webp ./docs/logo_3.webp", 
    "build:logo_4": "cp ./src/logo_4.webp ./docs/logo_4.webp", 
    "build:ico": "cp ./src/logo.ico ./docs/logo.ico", 
    "build:product_1": "cp ./src/product_1.webp ./docs/product_1.webp", 
    "build": "npm-run-all build:hero build:chatbot build:feature-1 build:feature-2 build:feature-3 build:css build:html build:logo build:logo_1 build:logo_2 build:logo_3 build:logo_4 build:product_1 build:ico", 
    "dev": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch"
  },
  "keywords": [],
