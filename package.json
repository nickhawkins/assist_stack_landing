  "main": "index.js",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/input.css -o ./docs/output.css --minify",
    "build:html": "cp ./src/index.html ./docs/index.html",
    "build:webp": "cp ./src/logo.webp ./docs/logo.webp",  
    "build:logo_1": "cp ./src/logo_1.webp ./docs/logo_1.webp",  
    "build:logo_2": "cp ./src/logo_2.webp ./docs/logo_2.webp", 
    "build:logo_3": "cp ./src/logo_3.webp ./docs/logo_3.webp", 
    "build:logo_4": "cp ./src/logo_4.webp ./docs/logo_4.webp", 
    "build:product_1": "cp ./src/product_1.webp ./docs/product_1.webp", 
    "build": "npm-run-all build:css build:html build:webp build:logo_1 build:logo_2 build:logo_4 build:product_1", 
    "dev": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch"
  },
